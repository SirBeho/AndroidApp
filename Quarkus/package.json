{
    "name": "quarkus-api",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "mvnw compile quarkus:dev",
        "build": "./mvnw clean package",
        "test": "./mvnw test",
        "clean": "./mvnw clean",
        "start": "java -jar target/quarkus-app/quarkus-run.jar",
        "docker-build": "docker build -t quarkus-api .",
        "docker-run": "docker run -d -p 9999:9999 --name quarkus-api quarkus-api",
        "rebuild": "npm run clean && npm run build && npm run start"
    },
    "dependencies": {
        "quarkus-api": "file:"
    }
}
